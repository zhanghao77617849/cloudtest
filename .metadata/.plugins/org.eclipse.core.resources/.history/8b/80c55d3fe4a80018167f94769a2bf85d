spring:
  application:
    name: config-client
  cloud:
    config:
      label: master  #远程仓库的分支
      profile: dev    #dev 开发环境配置文件  test 测试环境  pro 正式环境
      #uri: http://localhsot:8888/  #指明配置服务中心的网址
      discovery:
        enabled: true
        service-id: config-server

server:
  port: 8881 
  
eureka:
  client:
    service-url:
      defaultZone: http://localhost:8888/eureka/   